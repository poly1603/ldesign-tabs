{
  "name": "@ldesign/tabs",
  "version": "0.1.0",
  "description": "LDesign 页签系统 - 浏览器风格标签页组件，支持 Vue/React",
  "keywords": [
    "ldesign",
    "tabs",
    "page-tabs",
    "browser-tabs",
    "vue",
    "react",
    "typescript"
  ],
  "author": "LDesign Team",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.js",
      "require": "./lib/index.cjs"
    },
    "./core": {
      "types": "./es/core/index.d.ts",
      "import": "./es/core/index.js",
      "require": "./lib/core/index.cjs"
    },
    "./core/*": {
      "types": "./es/core/*.d.ts",
      "import": "./es/core/*.js",
      "require": "./lib/core/*.cjs"
    },
    "./react": {
      "types": "./es/react/index.d.ts",
      "import": "./es/react/index.js",
      "require": "./lib/react/index.cjs"
    },
    "./react/*": {
      "types": "./es/react/*.d.ts",
      "import": "./es/react/*.js",
      "require": "./lib/react/*.cjs"
    },
    "./types": {
      "types": "./es/types/index.d.ts",
      "import": "./es/types/index.js",
      "require": "./lib/types/index.cjs"
    },
    "./types/*": {
      "types": "./es/types/*.d.ts",
      "import": "./es/types/*.js",
      "require": "./lib/types/*.cjs"
    },
    "./utils": {
      "types": "./es/utils/index.d.ts",
      "import": "./es/utils/index.js",
      "require": "./lib/utils/index.cjs"
    },
    "./utils/*": {
      "types": "./es/utils/*.d.ts",
      "import": "./es/utils/*.js",
      "require": "./lib/utils/*.cjs"
    },
    "./vue": {
      "types": "./es/vue/index.d.ts",
      "import": "./es/vue/index.js",
      "require": "./lib/vue/index.cjs"
    },
    "./vue/*": {
      "types": "./es/vue/*.d.ts",
      "import": "./es/vue/*.js",
      "require": "./lib/vue/*.cjs"
    }
  },
  "main": "./lib/index.cjs",
  "module": "./es/index.js",
  "types": "./es/index.d.ts",
  "unpkg": "./dist/index.min.js",
  "jsdelivr": "./dist/index.min.js",
  "files": [
    "README.md",
    "LICENSE",
    "package.json",
    "es",
    "lib",
    "dist"
  ],
  "scripts": {
    "build": "ldesign-builder build",
    "build:watch": "ldesign-builder build --watch",
    "dev": "ldesign-builder build --mode development --watch",
    "clean": "rimraf es lib dist coverage",
    "prepublishOnly": "pnpm run clean && pnpm run build",
    "lint": "eslint . --fix",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@ldesign/shared": "workspace:*",
    "nanoid": "^5.0.9"
  },
  "peerDependencies": {
    "@ldesign/router": "^1.0.0",
    "@ldesign/color": "^1.0.0",
    "vue": "^3.3.0",
    "react": "^18.0.0"
  },
  "devDependencies": {
    "@ldesign/builder": "workspace:*",
    "@antfu/eslint-config": "^6.0.0",
    "eslint": "^9.18.0",
    "typescript": "^5.7.3",
    "rimraf": "^5.0.5"
  },
  "engines": {
    "node": ">=16.0.0",
    "pnpm": ">=8.0.0"
  },
  "peerDependenciesMeta": {
    "@ldesign/router": {
      "optional": true
    },
    "@ldesign/color": {
      "optional": true
    },
    "vue": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  }
}